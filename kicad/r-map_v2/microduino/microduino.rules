
(rules PCB microduino
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 568)
    (layer_rule F.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.1)
    )
    (layer_rule In1.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.9)
    )
  )
  (rule
    (width 254.0)
    (clear 254.2)
    (clear 127.0 (type smd_to_turn_gap))
    (clear 63.6 (type smd_smd))
  )
  (padstack "Via[0-1]_1035:635_um"
    (shape
      (circle F.Cu 1035.0 0.0 0.0)
    )
    (shape
      (circle In1.Cu 1035.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_1035:635_um" "Via[0-1]_1035:635_um" default
  )
  (via 
    "Via[0-1]_1035:635_um-kicad_default" "Via[0-1]_1035:635_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_1035:635_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_1035:635_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 254.0)
    )
    (circuit 
      (use_layer F.Cu In1.Cu)
    )
  )
  (class "kicad_default"
    "Net-(P2-Pad2)" "Net-(P2-Pad3)" "Net-(P2-Pad4)" "Net-(P2-Pad5)" "Net-(C1-Pad1)" "Net-(C2-Pad1)" "Net-(C3-Pad1)" "Net-(P3-Pad2)"
    "Net-(P3-Pad3)" "Net-(R1-Pad1)" "Net-(P4-Pad2)" GND "Net-(D1-Pad1)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 254.0)
    )
    (circuit 
      (use_layer F.Cu In1.Cu)
    )
  )
)